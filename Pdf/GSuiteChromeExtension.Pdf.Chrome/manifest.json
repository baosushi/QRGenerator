{
    "name": "PDF Converted",
    "version": "1.3",
    "manifest_version": 2,
    "description": "Quickly convert Document files to PDF.",
    "container": [ "GOOGLE_DRIVE", "DOMAIN_INSTALLABLE" ],
    "api_console_project_id": "8969790880",
    "gdrive_mime_types": {
        "http://drive.google.com/intents/opendrivedoc": [
            {
                "type": [
                    "application/vnd.google-apps.document",
                    "application/vnd.google-apps.kix",
                    "application/vnd.google.drive.ext-type.docx"
                ],
                "href": "https://pdfconverted.com/to-pdf",
                "title": "Open",
                "disposition": "window"
            }
        ]
    },
    "icons": {
        "128": "128.png",
        "64": "64.png",
        "32": "32.png"
    },
    "app": {
        "launch": {
            "web_url": "https://pdfconverted.com/to-pdf"
        },
        "urls": [
            "https://pdfconverted.com/to-pdf"
        ]
    }
}