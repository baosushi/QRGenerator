    @*<link async rel="stylesheet" href="~/css/design1.min.css" asp-append-version="true" />*@
    <link async rel="stylesheet" href="~/css/main.css" asp-append-version="true" />

    <title>@(this.ViewBag.Title)</title>

    @this.RenderSection("Heads", false)
</head>
<body lang="@(this.texts.GetLanguageCode())">

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=123"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', '123');
    </script>

