{
    "name": "Free Gantt Chart",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Create professional Gantt Chart to manage your work task for free.",
    "container": [ "GOOGLE_DRIVE", "DOMAIN_INSTALLABLE" ],
    "api_console_project_id": "429665447038",
    "gdrive_mime_types": {
        "http://drive.google.com/intents/opendrivedoc": [
            {
                "type": [
                    "application/x-shockwave-flash"
                ],
                "href": "https://gantt.freebusinessapps.net/gantt-chart",
                "title": "Open",
                "disposition": "window"
            }
        ]
		
    },
    "icons": {
        "512": "logo_512.png",
        "256": "logo_256.png",
        "128": "logo_128.png",
        "96": "logo_96.png",
        "64": "logo_64.png",
        "48": "logo_48.png",
        "32": "logo_32.png",
        "16": "logo_16.png"
    },
    "browser_action": {
        "default_icon": {
            "512": "logo_512.png",
			"256": "logo_256.png",
			"128": "logo_128.png",
			"96": "logo_96.png",
			"64": "logo_64.png",
			"48": "logo_48.png",
			"32": "logo_32.png",
			"16": "logo_16.png"
        }
    },
	"background": {
		"scripts": [ "launch.js" ]
	}
}